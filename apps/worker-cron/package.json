{
  "name": "worker-cron",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "npm run prisma:generate && nodemon --watch 'src/**/*.ts' --exec ts-node src/index.ts",
    "prisma:generate": "npm run generate --workspace=@tutorr/db",
    "build": "tsc",
    "start": "node dist/index.js",
    "start-worker": "nodemon --watch src --exec ts-node src/scripts/start.ts",
    "stop-worker": "ts-node src/scripts/close.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@tutorr/common": "*",
    "arg": "^4.1.3",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "make-error": "^1.3.6",
    "nodemon": "^3.1.10",
    "socket.io": "^4.8.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.3"
  }
}